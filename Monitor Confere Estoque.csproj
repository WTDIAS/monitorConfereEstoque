<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Monitor_Confere_Estoque</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>CE64X64.ico</ApplicationIcon>
    <Company>GigaLike Sistemas</Company>
    <Description>Este software faz parte do APP (Android) Confere Estoque. Após fazer as coletas de codigos de barras e quantidade no APP Confere Estoque e clicar em backup este software fará a leitura destes dados gerando um arquivo .txt na pasta escolhida.</Description>
    <PackageIcon>CE64X64.ico</PackageIcon>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FireSharp" Version="2.0.4" />
    <PackageReference Include="Twileloop.ExpressSecurity" Version="1.0.0" />
  </ItemGroup>

</Project>